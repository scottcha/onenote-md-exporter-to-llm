{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "msbuild",
            "program": "${workspaceFolder}/src/OneNoteMdExporter/bin/Debug/net8.0-windows/win-x86/OneNoteMdExporter.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/OneNoteMdExporter/",
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}